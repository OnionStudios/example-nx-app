  "scripts": {
    "start": "nx serve",
    "start:api": "nx run api:serve",
    "start:web": "nx run web:serve",
    "format:write": "nx format:write",
    "build": "nx build",
    "test": "nx test",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "npm run typeorm -- migration:create",
    "typeorm:migration": "node tools/cli/gen-migration.mjs --name",
    "typeorm:up": "npm run typeorm -- --dataSource=./data-source.ts migration:run",
    "typeorm:down": "npm run typeorm -- --dataSource=./data-source.ts migration:revert",
    "typeorm:show": "npm run typeorm -- --dataSource=./data-source.ts migration:show",
    "shopify": "shopify",
    "update-url": "node tools/cli/update.url.mjs",
    "prepare": "husky install"
  },
